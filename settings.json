{ // Place your settings in this file to overwrite the default settings
    "vim.useCtrlKeys": true,
    "vim.easymotion": true,
    "editor.fontFamily": "Dank Mono",
    "explorer.openEditors.visible": 0,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "vim.enableNeovim": true,
    // "vim.neovimPath": "f:\\program files\\Neovim\\bin\\nvim.exe",
    "vim.foldfix": true,
    "vim.handleKeys": {
        "<C-d>": false
    },
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "vim.sneak": true,
    "vim.insertModeKeyBindings": [],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.quickOpen",
        "workbench.action.openEditorAtIndex1",
        "workbench.action.openEditorAtIndex2",
        "editor.action.outdentLines",
        "workbench.action.openEditorAtIndex3",
        "workbench.action.openEditorAtIndex4",
        "workbench.action.showCommands",
        "workbench.action.terminal.clear",
        "workbench.action.terminal.copySelection",
        "workbench.action.terminal.focus",
        "workbench.action.terminal.focusNext",
        "workbench.action.terminal.focusPrevious",
        "workbench.action.terminal.kill",
        "workbench.action.terminal.new",
        "workbench.action.terminal.paste",
        "workbench.action.terminal.runSelectedText",
        "workbench.action.terminal.scrollDown",
        "workbench.action.terminal.scrollDownPage",
        "workbench.action.terminal.scrollUp",
        "workbench.action.terminal.scrollUpPage",
        "workbench.action.terminal.toggleTerminal"
    ],
    //standard settings
    "javascript.validate.enable": false,
    "standard._legacyModuleResolve": true,
    "standard.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "html",
            "autoFix": true
        }
    ],
    "standard.options": {
        "plugins": [
            "html"
        ]
    },
    "files.associations": {
        "*.vue": "html"
    },
    "window.zoomLevel": 0,
    "terminal.integrated.shellArgs.windows": [
        "/k",
        "e://users//Saleh//Downloads//cmder_mini//vendor//init.bat"
    ],
    "terminal.integrated.shell.windows": "cmd.exe",
    "window.enableMenuBarMnemonics": false,
    "vim.leader": ",",
    "workbench.activityBar.visible": true,
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "vim.disableExtension": false,
    "window.menuBarVisibility": "toggle",
    "terminal.integrated.rendererType": "dom",
    "breadcrumbs.enabled": false,
    "workbench.colorTheme": "Material Theme",
    "debug.node.autoAttach": "on"
}